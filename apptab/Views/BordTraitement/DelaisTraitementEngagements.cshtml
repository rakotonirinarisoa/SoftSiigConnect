@{
    ViewBag.Title = "Suvi des délais de traitement des engagements";
    ViewData["Controller"] = "Suvi des délais de traitement des engagements";
}

<table id="dashboard" class="table table-hover table-striped table-bordered" display responsive nowrap" width="100%">
    <thead style="position:sticky">
        <tr class="thead-accueil1" style="white-space: nowrap;">
            <th style="font-weight: bold; text-align: center">SOA</th>
            <th style="font-weight: bold; text-align: center">Num Engagement</th>
            <th style="font-weight: bold; text-align: center">Etape</th>
            <th style="font-weight: bold; text-align: center">Bénéficiaire</th>
            <th style="font-weight: bold; text-align: center">Montant</th>
            <th style="font-weight: bold; text-align: center">Agent</th>
            <th style="font-weight: bold; text-align: center">Date de traitement</th>
            <th style="font-weight: bold; text-align: center">Durée de traitement</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>

<div class="form-row">
    <div class="input-group mb-3 col">
        <div class="btn btn-flat bg-gradient-yellow col-12" id="export-excel-btn">
            <i class="fa fa-file-excel"></i> Exporter
        </div>
    </div>
</div>

<script src="~/Assets/js/utils.js"></script>
